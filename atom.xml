---
layout: nil
url: atom.xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>DailyJS</title>
 <link href="http://phpsunday.com/feed.xml" rel="self"/>
 <link href="http://phpsunday.com" />
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://phpsunday.com/</id>
 <author>
   <name>PHPSunday</name>
 </author>
 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://phpsunday.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://phpsunday.com{{ post.id }}</id>
   <content type="html">
     {{ post.content | xml_escape }}
   </content>
 </entry>
 {% endfor %}
</feed>
